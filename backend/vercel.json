{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js",
            "methods": ["GET", "HEAD", "PUT", "POST", "PATCH", "DELETE"],
            "headers":{
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]


}